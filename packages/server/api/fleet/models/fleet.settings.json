      "collection": "service"
    },
    "apply": {
      "type": "boolean",
      "default": true
    },
    "template": {
      "via": "fleets",
      "model": "template"
    },
    "version": {
      "type": "integer",
      "default": 1
    },
    "servicesSidecar": {
      "via": "sidecar",
      "collection": "service"
    },
    "status": {
      "type": "string"
    },
    "unhealthy": {
      "type": "json"
    },
    "meshes": {
      "collection": "mesh",
      "via": "prometheus"
    }
