{
  "kind": "collectionType",
  "collectionName": "fleets",
  "info": {
    "name": "Fleet",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "type": {
